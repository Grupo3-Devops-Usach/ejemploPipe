pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                bat "mvn clean -e"
            }
        }

        stage('test') {
            steps {
                bat "mvn clean compile -e"
            }
        }

        stage('test2') {
            steps {
                bat "mvn test -e"
                
            }
        }
    }
}