pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh "/usr/share/maven/mvn clean"
            }
        }

        stage('test') {
            steps {
                sh "/usr/share/maven/mvn compile"
            }
        }

        stage('test2') {
            steps {
                sh '''
                    /usr/share/maven/mvn test
                '''
            }
        }
    }
}
