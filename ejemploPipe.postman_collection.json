					"listen": "test",
					"script": {
						"exec": [
							"const responseJson = pm.response.json();\r",
							"\r",
							"pm.test(\"Sataus code is 200\", function(){\r",
							"    pm.response.to.have.status(200)\r",
							"});\r",
							"\r",
							"pm.test(\"Response time is less than 200ms\", () =>{\r",
							" pm.expect(pm.response.responseTime).to.be.below(1200);\r",
							"});\r",
							"\r",
							"pm.test(\"Codigo content uf\",() => {\r",
							"  pm.expect(responseJson.codigo).to.eql(\"uf\");\r",
							"});\r",
							"\r",
							"pm.test(\"Content-Type header is present\", () => {\r",
							"  pm.response.to.have.header(\"Content-Type\");\r",
							"});\r",
							"\r",
							"pm.test(\"nombre es Unidad de fomento (UF), unidad de medida Pesos\", () => {\r",
							"  const responseJson = pm.response.json();\r",
							"  pm.expect(responseJson.nombre).to.eql(\"Unidad de fomento (UF)\");\r",
							"  pm.expect(responseJson.unidad_medida).to.eql(\"Pesos\");\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
