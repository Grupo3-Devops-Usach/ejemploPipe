pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh "./mvnw clean -e"
            }
        }

        stage('test') {
            steps {
                sh "./mvnw compile -e"
            }
        }

        stage('test2') {
            steps {
                sh "./mvnw test-e"
            }
        }
    }
}